    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'chimera'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=chimera",
                    "--package=chimera"
                ],
                "filter": {
                    "name": "chimera",
                    "kind": "bin"
                }
            },
            "args": [
                // "-i", "local/chimera/old/new-slide-example.fastq",
                "-i", "local/chimera/first-run/PE_CRISPRlib_5_S8_L001_R1_001/10000/chimera/input.fastq",
                "-r", "local/chimera/first-run/CRISPR_primeediting_guides_Miles_MyD88_oligos.onlypegRNA-ngRNA_forchimerasupper.txt"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'chimera'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=chimera",
                    "--package=chimera"
                ],
                "filter": {
                    "name": "chimera",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}